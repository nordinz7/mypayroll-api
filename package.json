{
  "name": "api",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "dev:prod": "bun run src/index.ts",
    "build:ts": "rm -rf dist && tsc",
    "watch:ts": "tsc -w",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "eslint --ext .ts --fix src/",
    "generate-types": "graphql-code-generator --config codegen.ts",
    "test": "bun test --coverage --watch NODE_ENV=test",
    "test:single": "bun test --watch ./src/tests/files/user.test.ts NODE_ENV=test"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "bullmq": "^5.29.1",
    "date-fns": "^3.6.0",
    "graphql-apollo-errors": "^2.0.3",
    "graphql-yoga": "^5.1.1",
    "ioredis": "^5.4.1",
    "joi": "^17.13.1",
    "jsonwebtoken": "9.0.2",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.1",
    "sequelize-cli": "^6.6.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.4",
    "@graphql-codegen/typescript-resolvers": "^4.0.4",
    "@graphql-tools/load-files": "^7.0.0",
    "@types/bcryptjs": "2.4.6",
    "@types/bun": "latest",
    "@types/jsonwebtoken": "9.0.6",
    "@types/lodash": "^4.17.1",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.0.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}